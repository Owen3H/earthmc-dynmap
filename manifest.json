{
	"manifest_version": 3,
	"name": "EarthMC Dynmap+",
	"version": "1.17",
	"author": "3meraldK",
	"description": "Extension to enrich the EarthMC map experience",
	"icons": {
		"48": "icon.png"
	},

    "web_accessible_resources": [{
        "resources": ["main.js"],
        "matches": ["https://map.earthmc.net/*"]
    }],

	"content_scripts": [{
		"matches": ["https://map.earthmc.net/*"],
		"js": ["httputil.js", "content.js"],
		"css": ["style.css"]
	}]
}