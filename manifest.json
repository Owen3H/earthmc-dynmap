{
	"manifest_version": 3,
	"name": "EarthMC Dynmap+ (Updated)",
	"version": "1.16",
	"author": "3meraldK",
	"description": "Extension to enrich the EarthMC map experience",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"web_accessible_resources": [{
		"run_at": "document_start",
		"matches": ["https://map.earthmc.net/*"],
		"resources": ["src/interceptor.js"]
	}],
	"content_scripts": [{
		"matches": ["https://map.earthmc.net/*"],
		"css": ["style.css"],
		"js": [
			"src/httputil.js",
			"src/dom.js",
			"src/main.js",
			"src/content.js"
		]
	}]
}