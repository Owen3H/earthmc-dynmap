{
	"manifest_version": 3,
	"name": "EarthMC Dynmap+",
	"version": "1.16",
	"author": "3meraldK",
	"description": "Extension to enrich the EarthMC map experience",
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"web_accessible_resources": [{
		"matches": ["https://map.earthmc.net/*"],
		"resources": ["interceptor.js"],
		"run_at": "document_start"
	}],
	"content_scripts": [{
		"matches": ["https://map.earthmc.net/*"],
		"js": ["httputil.js", "dom.js", "main.js", "content.js"],
		"css": ["style.css"]
	}]
}